{
  "name": "study-assist-feedback",
  "version": "1.0.0",
  "description": "学习助手反馈系统 - Cloudflare Workers + KV",
  "main": "workers/feedback-api.js",
  "scripts": {
    "dev": "wrangler dev",
    "dev:feedback": "wrangler dev --config wrangler.toml",
    "dev:glm": "wrangler dev --config wrangler-glm.toml",
    "deploy": "wrangler deploy --config wrangler.toml",
    "deploy:site": "wrangler deploy --config wrangler-site.toml",
    "deploy:feedback": "wrangler deploy --config wrangler.toml",
    "deploy:glm": "wrangler deploy --config wrangler-glm.toml",
    "tail": "wrangler tail --config wrangler.toml",
    "tail:glm": "wrangler tail --config wrangler-glm.toml",
    "test": "wrangler dev --local --config wrangler.toml",
    "test:glm": "wrangler dev --local --config wrangler-glm.toml"
  },
  "keywords": [
    "cloudflare",
    "workers",
    "kv",
    "feedback",
    "education"
  ],
  "author": "Study Assist Team",
  "license": "MIT",
  "devDependencies": {
    "wrangler": "^3.0.0"
  }
}